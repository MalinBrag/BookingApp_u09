{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "database-api",
      "urls": [
        "/api/**",
        "https://backend-production-a88c.up.railway.app/api/*"
      ],
      "cacheConfig": {
        "strategy": "staleWhileRevalidate",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "10s"
      }
    },
    {
      "name": "amadeus-api",
      "urls": [
        "https://test.api.amadeus.com/v1/**"
      ],
      "cacheConfig": {
        "strategy": "staleWhileRevalidate",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "10s"
      }
    }
  ]
}
